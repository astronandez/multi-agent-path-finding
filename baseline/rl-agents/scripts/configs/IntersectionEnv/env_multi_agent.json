{
    "id": "intersection-multi-agent-v0",
    "import_module": "highway_env",
    "observation": {
        "type": "MultiAgentObservation",
        "observation_config": {
            "type": "Kinematics",
            "vehicles_count": 5,
            "features": ["presence", "x", "y", "vx", "vy", "cos_h", "sin_h"],
            "features_range": {
                "x": [-100, 100],
                "y": [-100, 100],
                "vx": [-20, 20],
                "vy": [-20, 20]
            },
            "absolute": true,
            "order": "shuffled"
        }
    },
    "initial_vehicle_count": 2,
    "controlled_vehicles": 2,
    "spawn_probability": -1,
    "destination": null,
    "collision_reward": -10
}